package display.screen{	import flash.display.MovieClip;	import flash.text.TextField;		public class Lose extends Screen{				private var label:TextField;				public function Lose(root:TowerDefense){						super(root);						graphics.beginFill(0xFFFFFF);			graphics.drawRect(0,0,9000,9000);			graphics.endFill();						label = new TextField();			label.x = 80;			label.y = 150;			label.width = 390;			label.text = 'Uh oh! You\'ve lost all of your lives!';			addChild(label);		}	}}